      "outDir": "./bin/web/wwwroot",
      "assets": [
        "./src/web/wwwroot/assets",
        "./src/web/wwwroot/favicon.ico"
      ],
      "index": "./src/web/wwwroot/index.html",
      "main": "./src/web/wwwroot/main.ts",
      "polyfills": "./src/web/wwwroot/polyfills.ts",
      "test": "./src/web/wwwroot/test.ts",
      "tsconfig": "./src/web/wwwroot/tsconfig.app.json",
      
      "prefix": "app",
      "styles": [
        "./src/web/wwwroot/styles.css"
      ],
      "scripts": [],
      "environmentSource": "./src/web/wwwroot/environments/environment.ts",
      "environments": {
        "dev": "./src/web/wwwroot/environments/environment.ts",
        "prod": "./src/web/wwwroot/environments/environment.prod.ts"
      }
